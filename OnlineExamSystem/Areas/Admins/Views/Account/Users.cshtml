@using global::OnlineExamSystem.Models
@model IEnumerable<ApplicationUser>

<h2>Users</h2>

<table class="table">
    <thead>
        <tr>
            <th>Username</th>
            <th>Email</th>
           <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserName</td>
                <td>@user.Email</td>
                 <td>
                    <a href="@Url.Action("EditUser", "Admin", new { id = user.Id })">Edit</a> |
                    <a href="@Url.Action("DeleteUser", "Admin", new { id = user.Id })">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>

<a href="@Url.Action("AddUser", "Admin")">Add New User</a>
